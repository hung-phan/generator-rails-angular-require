<% extend JasmineRails::SpecHelper %>
define [
  "angular"
], (angular) ->
  <% templates.each do |module_name, path| %>
  angular.module("<%= module_name %>", []).run ["$templateCache", ($templateCache) ->
    $templateCache.put "<%= path %>", """<%= asset_to_s module_name  %>"""
  ]
  <% end %>
  return
